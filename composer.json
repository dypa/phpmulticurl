{
    "name": "dypa/phpmulticurl",
    "description": "Ultra fast non-blocking OOP wrapper for curl_multi_* functions.",
    "keywords": ["curl", "curl_multi_select", "multicurl", "phpmulticurl"],
    "license": "MIT",
    "authors": [
        {
            "name": "dypa",
            "email": "dypa@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=7.3.10",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "PhpMultiCurl\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/process": "^5.0"
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
